<% layout('./layout/main') -%>
<div class="container-fluid p-0">
  <div id="carouseExampleIndicators" class="carousel slide" data-ride="carousel">
    <ol class="carousel-indicators">
      <li class="active" data-target="#carouseExampleIndicators" data-slide-to="0"></li>
      <li data-target="#carouseExampleIndicators" data-slide-to="1"></li>
      <li data-target="#carouseExampleIndicators" data-slide-to="2"></li>
    </ol>
    <div class="carousel-inner">
      <div class="carousel-item active">
        <img src="/images/1.jpeg" alt="" class="d-block w-100">
        <div class="carousel-caption">
          <h1>Детская группа</h1>
          <p>Джиу-джитсу, панкратион</p>
        </div>
      </div>
      <div class="carousel-item">
        <img src="/images/2.jpeg" alt="" class="d-block w-100">
        <div class="carousel-caption">
          <h1>Выступления</h1>
          <p>парам пам пам</p>
        </div>
      </div>
      <div class="carousel-item">
        <img src="/images/3.jpeg" alt="" class="d-block w-100">
        <div class="carousel-caption">
          <h1>Взрослая группа</h1>
          <p>Джиу-джитсу, ММА</p>
        </div>
      </div>
    </div>
    <a href="#carouseExampleIndicators" class="carousel-control-prev" role="button" data-slide="prev">
      <span class="carousel-control-prev-icon" aria-hidden="true"></span>
      <span class="sr-only">Previous</span>
    </a>
    <a href="#carouseExampleIndicators" class="carousel-control-next" role="button" data-slide="next">
      <span class="carousel-control-next-icon" aria-hidden="true"></span>
      <span class="sr-only">Next</span>
    </a>
  </div>
</div>
